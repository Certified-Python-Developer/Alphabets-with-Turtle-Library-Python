import turtle
turtle.bgcolor("black")
turtle.penup()
turtle.goto(-80,0)
turtle.pendown()
turtle.color("yellow")
turtle.pendown()
turtle.goto(-160,0)
turtle.penup()
turtle.goto(-160,40)
turtle.pendown()
turtle.goto(-80,40)
turtle.color("white")
turtle.penup()
turtle.goto(-160,40)
turtle.pendown()
turtle.goto(-160,0)
turtle.penup()
turtle.goto(-160,40)
turtle.pendown()
turtle.goto(-160,80)
turtle.penup()
turtle.goto(-80, 80)
turtle.pendown()
turtle.goto(-160,80)
