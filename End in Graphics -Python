import turtle
turtle.penup()
turtle.goto(-30,0)
turtle.pendown()
turtle.goto(-30,80)
turtle.penup()
turtle.goto(40, 0)
turtle.pendown()
turtle.goto(40,80)
turtle.penup()
turtle.goto(-30,80)
turtle.pendown()
turtle.goto(40,0)
turtle.goto(80,0)



import turtle
turtle.speed(0)
turtle.penup()
turtle.goto(80,80)
turtle.pencolor("yellow")
turtle.pendown()
turtle.goto(80,0)

import turtle
turtle.speed(0)
turtle.circle(40,185)
